{
    "greeting": {
        "patterns": [
            "Hi",
            "Hello"
        ],
        "responses": [
            "Hello, how can I help you?",
            "Good day! How can I help you?"
        ]
    },
    "bye": {
        "patterns": [
            "Bye",
            "Goodbye",
